        for g, j in pairs(v:GetChildren()) do
            if j:IsA("Model") and j.PrimaryPart then
                local Color = j.PrimaryPart:GetAttribute("exploitingisevil") 
                    and Color3.fromRGB(248, 87, 87) 
                    or Color3.fromRGB(28, 235, 87)
