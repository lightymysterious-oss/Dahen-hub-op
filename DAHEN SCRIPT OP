local skySquidQTEConnection = nil

SkySquidLeftGroupBox:AddToggle("SkySquidAutoQTE", {
    Text = "Auto QTE - Sky Squid",
    Default = false,
    Callback = function(Value)
        isSkySquidQTEActive = Value
        
